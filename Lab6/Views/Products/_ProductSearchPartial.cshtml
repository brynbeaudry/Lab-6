@model IEnumerable<Lab6.Models.NorthWind.Product>

<div id="searchresults">
    @if(Model.Count() == 0)
    {
        <h4 style="color: #ff0000">Not Found</h4>
    }
    else { 
        <ul>
            @foreach (var item in Model) { 
                <li>@item.ProductName</li>
            }
        </ul>
    }
</div>
